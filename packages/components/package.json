{
  "name": "maximum-ui",
  "version": "0.0.0",
  "type": "module",
  "sideEffects": false,
  "scripts": {
    "build": "tsc --noEmit && vite build",
    "build:analyze": "tsc --noEmit && vite build --mode analyze",
    "dev:storybook": "storybook dev -p 6006",
    "build:storybook": "storybook build"
  },
  "dependencies": {
    "clsx": "^1.2.1",
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.14",
    "@vitejs/plugin-react": "^4.0.1",
    "rollup-plugin-visualizer": "^5.9.2",
    "sass": "^1.63.6",
    "vite": "^4.3.9",
    "vite-plugin-dts": "3.0.0-beta.3"
  },
  "keywords": [
    "react",
    "react-component",
    "ui",
    "component"
  ],
  "main": "./dist/maximum-ui.umd.js",
  "module": "./dist/maximum-ui.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/maximum-ui.es.js",
      "require": "./dist/maximum-ui.umd.js"
    },
    "./style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "files": [
    "dist"
  ],
  "author": "sor4chi",
  "license": "MIT"
}
